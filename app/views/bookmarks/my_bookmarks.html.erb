<div class="container">
  <div class="row">
    <h1>My Bookmarks</h1>
      <% current_user.bookmarks.each do |bookmark| %>
        <h3><%= link_to "#{bookmark.url}", bookmark.url %>
        <%#= render partial: 'likes/like', collection: @bookmarks, locals: { user_id: current_user.id} %></h3>
      <% end %>
      <% unless current_user.bookmarks.present? %>
        <h3>You do not have any bookmarks. Browse topics to add a bookmark</h3>
      <% end %>
      <%= link_to "View Topics", topics_path, class: 'btn btn-info btn-bottom' %> <%= link_to "All Bookmarks", bookmarks_path, class: 'btn btn-info btn-bottom' %>
  </div>
</div>